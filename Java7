import java.util.Scanner;
public class dialogue{
	public static void main (String[] args){

	Scanner s = new Scanner(System.in);
	  System.out.println("Wie lautet ihr Name? ");
	  
	String wohnort = s.next();
	  System.out.println("In welchem Land wohnen Sie?");
	    
	String name = s.next(); 
	  System.out.println("Ihr Alter: ");
	  
	int alter = s.nextInt(); 
	  if(alter>=18)
	     System.out.println("Glückwunsch, Sie sind älter als ich!");
	     System.out.println("Sie sind "+alter+ ", wohnen in "+name+ " und heißen "+wohnort+"! Richtig? ");
	 
	String eingabe = s.next();
	  System.out.println("\n");
	  System.out.println("Ok!!");
	   
	
}
}
