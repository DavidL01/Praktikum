public class numbers {
  public static void main(String[] args) {
   
  
  	 // Variable i wird deklariert und initialisiert
	// Abbruchbedingung wird festgelegt auf i < 14
	// i wird nach jedem durchlauf um eins erhÃ¶ht
	for(int i=0; i<14; i++)
	{
  	
  	
	if(i==8) {

	break;
        }

	// Die Ausgabe findet achtmal statt (von 0 bis 8)
    	System.out.println("i ist "+i);
	}

     }

  }
